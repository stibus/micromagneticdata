OutputFormat = OVF2_BINARY

// Mesh
SetGridSize(20, 10, 4)
SetCellSize(5e-09, 5e-09, 5e-09)

// Magnetisation
m.LoadFile("/home/martin/PhD/repos/ubermag-devtools/repos/micromagneticdata/micromagneticdata/tests/test_sample/system_name/drive-1/m0.omf")
Msat.setregion(0, 799999.9999999999)

regions.LoadFile("/home/martin/PhD/repos/ubermag-devtools/repos/micromagneticdata/micromagneticdata/tests/test_sample/system_name/drive-1/mumax3_regions.omf")

// Exchange energy
Aex = 1.3e-11
tableadd(E_exch)
// Zeeman
B_ext.add(LoadFile("B_ext.ovf"), 1)
tableadd(E_Zeeman)
enabledemag = false

tableadd(E_total)
tableadd(dt)
tableadd(maxtorque)
alpha = 0.02
gammaLL = 176085964286.56906
doprecess = true
setsolver(5)
fixDt = 0

for snap_counter:=0; snap_counter<15; snap_counter++{
    run(1e-12)
    save(m_full)
    tablesave()
}