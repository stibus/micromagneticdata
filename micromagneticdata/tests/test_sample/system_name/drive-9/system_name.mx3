// Mesh
SetGridSize(20, 20, 1)
SetCellSize(5e-09, 5e-09, 5e-09)

// Magnetisation
m.LoadFile("m0.omf")
Msat.setregion(0, 800000.0000000003)

regions.LoadFile("mumax3_regions.omf")

// Exchange energy
Aex = 1.3e-11
tableadd(E_exch)
// Demag
enabledemag = true
tableadd(E_demag)
// Zeeman
B_ext = vector(0.0, 0.0, 0.0)
tableadd(E_Zeeman)
tableadd(E_total)
tableadd(dt)
tableadd(maxtorque)
alpha = 0.2
gammaLL = 176085964286.56906
doprecess = true
setsolver(5)
fixDt = 0

for snap_counter:=0; snap_counter<10; snap_counter++{
    run(5e-10)
    save(m_full)
    tablesave()
}